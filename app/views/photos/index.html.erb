<!DOCTYPE html>

<h1 id="page_title">Photography</h1>
<div id="gallery">
  <div class="wrapper clearfix">
    <% @photos.each do |photo| %>
      <%= link_to(image_tag(photo.image.url(:medium)), (photo.image.url(:large)), data: { lightbox: "example-set", title: photo.name, rel: 'lightbox' }) %>
    <% end %>
  </div>
</div>
